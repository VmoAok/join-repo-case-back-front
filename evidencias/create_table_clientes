CREATE TABLE IF NOT EXISTS db_join.tb_jiki_registro_clientes
(
    id_usuario character varying(10) COLLATE pg_catalog."default",
    tipo_usuario character varying(10) COLLATE pg_catalog."default",
    id_documento character varying(20) COLLATE pg_catalog."default",
    cpf character varying(14) COLLATE pg_catalog."default",
	nome_completo character varying(150) COLLATE pg_catalog."default",
	data_nascimento date,
    email character varying(100) COLLATE pg_catalog."default",
    telefone character varying(20) COLLATE pg_catalog."default",
    endereco character varying(255) COLLATE pg_catalog."default",
    cidade character varying(100) COLLATE pg_catalog."default",
    estado character(2) COLLATE pg_catalog."default",
    cep character varying(10) COLLATE pg_catalog."default",
    data_inclusao date,
    data_atualizacao date
)
