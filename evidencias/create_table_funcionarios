-- Table: db_join.tb_jiki_registro_funcionarios

CREATE TABLE IF NOT EXISTS db_join.tb_jiki_registro_funcionarios
(
    id_usuario character varying(10) COLLATE pg_catalog."default",
    tipo_usuario character varying(10) COLLATE pg_catalog."default",
    numero_matricula integer,
    nome_funcionario character varying(100) COLLATE pg_catalog."default",
    endereco_funcionario character varying(200) COLLATE pg_catalog."default",
    telefone_funcionario character varying(20) COLLATE pg_catalog."default",
    data_registro_funcionario date,
    funcionario_ativo character(1) COLLATE pg_catalog."default",
    data_desligamento date,
    email_funcionario character varying(100) COLLATE pg_catalog."default"
)
